<h4 class="text-center">Subjects (<%=subjects.length %>)</h4>
<table class="table table-striped">
    <tbody>
        <% if (subjects != undefined) {
    subjects.forEach((s) => { %>
        <tr>
            <td><a href="/r/<%= s.name %>"><%= s.name %></a></td>
        </tr>
        <% }); %>
        <% if(subjects.length == 0) { %>
        You are not subscribed to any Subject
        <% } %>
        <% } %>
    </tbody>
</table>